{{!--generates html code for users and generates elements for expenses --}}
{{#each users as |user| }}
<div class="row align-center mb-5">
  <div class="col-md-6">
    <h2>{{!{{user.name}}</h2>
  </div>
  <div class="col-md-6">
    <h3><a href="mailto:{{!{{user.email}}">{{!{{user.email}}</a></h3>
  </div>
</div>
<button id="logout" class="btn-no-style">Logout</button>
{{/each}}
{{#each expenses.expenses as |expense| }}
<h4>{{expense.name}}: ${{expense.cost}}</h4>
{{/each}}

